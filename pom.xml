<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>gov.nsa.datawave.microservice</groupId>
        <artifactId>datawave-microservice-service-parent</artifactId>
        <version>5.0.1-SNAPSHOT</version>
        <relativePath>../../microservice-service-parent/pom.xml</relativePath>
    </parent>
    <artifactId>spring-boot-starter-datawave</artifactId>
    <version>4.0.3-SNAPSHOT</version>
    <url>https://code.nsa.gov/datawave-spring-boot-starter</url>
    <licenses>
        <license>
            <name>The Apache License, Version 2.0</name>
            <url>https://www.apache.org/licenses/LICENSE-2.0.txt</url>
        </license>
    </licenses>
    <scm>
        <connection>scm:git:https://github.com/NationalSecurityAgency/datawave-spring-boot-starter.git</connection>
        <developerConnection>scm:git:git@github.com:NationalSecurityAgency/datawave-spring-boot-starter.git</developerConnection>
        <tag>HEAD</tag>
        <url>https://github.com/NationalSecurityAgency/datawave-spring-boot-starter</url>
    </scm>
    <properties>
        <version.accumulo>2.1.1</version.accumulo>
        <version.beanutils>1.9.4</version.beanutils>
        <version.dropwizard-metrics>4.1.2</version.dropwizard-metrics>
        <version.in-memory-accumulo>4.0.0</version.in-memory-accumulo>
        <version.jackson>2.10.1</version.jackson>
        <version.jzlib>1.1.3</version.jzlib>
        <version.metrics-spring>3.1.3</version.metrics-spring>
        <version.microservice.accumulo-utils>4.0.0</version.microservice.accumulo-utils>
        <version.microservice.authorization.api>4.0.1</version.microservice.authorization.api>
